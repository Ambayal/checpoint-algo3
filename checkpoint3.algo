PROCEDURE insertion_sort (vab tab, tabsort : ARRAY_OF INTEGER)
VAR
    i, j : INTEGER;
BEGIN
    FOR i FROM 1 TO tab.length-1
        tabsort = tab[i]
        i = j
        IF j > 0 and tab[j-1] > tabsort THEN
            tab[j-1] = tab[j]
            j-1 = j
            tabsort = tab[j]
        END_IF
    END_FOR
END 
